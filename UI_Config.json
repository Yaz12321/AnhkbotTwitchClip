    "tooltip": "The command to like",
  	"group": "General"
  },
    "Permission": {
    "type": "dropdown",
    "value": "Caster",
    "items": ["Everyone","Regular","Subscriber","GameWisp Subscriber","User_Specific","Min_Rank","Min_Points","Min_Hours","Moderator","Editor","Caster"],
